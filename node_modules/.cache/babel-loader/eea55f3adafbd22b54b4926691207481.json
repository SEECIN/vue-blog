Vuescroll :ops=\"ops\">\r\n      <van-image v-if=\"!this.$store.state.token\"\r\n                 round\r\n                 width=\"5rem\"\r\n                 height=\"5rem\"\r\n                 :src=\"require('@/assets/img/logo.jpg')\" />\r\n      <van-card v-else\r\n                class=\"blogm-item\">\r\n        <template #thumb>\r\n          <van-image round\r\n                     width=\"80\"\r\n                     height=\"80\"\r\n                     fit=\"cover\"\r\n                     class=\"blogm-card--image\"\r\n                     :src=\"avatar\" />\r\n        </template>\r\n        <template #title>\r\n          <h3 class=\"van-ellipsis blogm-item--title\">{{nikname}}</h3>\r\n        </template>\r\n        <template #desc>\r\n          <p class=\"van-ellipsis\">\r\n            {{signature}}\r\n          </p>\r\n        </template>\r\n      </van-card>\r\n      <BaseForm />\r\n      <div class=\"blogm-logout--wrap\">\r\n        <van-button v-if=\"this.$store.state.token\"\r\n                    class=\"blogm-btn--logout\"\r\n                    type=\"primary\"\r\n                    @click=\"logOut\"\r\n                    block>退出登录</van-button>\r\n      </div>\r\n    </Vuescroll>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Vuescroll from 'vuescroll'\r\nimport { mapGetters, mapActions } from 'vuex'\r\nimport BaseForm from '@/components/m/BaseForm'\r\nexport default {\r\n  name: 'User',\r\n  components: {\r\n    BaseForm,Vuescroll\r\n  },\r\n  inject: ['reload'],\r\n  data () {\r\n    return {\r\n      isLogin: false,\r\n       ops: {\r\n        bar: {\r\n          background: '#c1c1c1',\r\n          disabled:true\r\n        }\r\n      }\r\n    }\r\n  },\r\n  mounted () {\r\n\r\n  },\r\n  computed: {\r\n    avatar () {\r\n      return this.userInfo?.avatar\r\n    },\r\n    nikname () {\r\n      return this.userInfo?.nikname\r\n    },\r\n    signature () {\r\n      return this.userInfo?.signature\r\n    },\r\n    ...mapGetters(['userInfo']