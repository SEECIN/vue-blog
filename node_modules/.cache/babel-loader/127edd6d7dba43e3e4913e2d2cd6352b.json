ard class=\"blog-editor\">\r\n    <h3 class=\"blog-editor-title\">标题</h3>\r\n    <el-input ref=\"title\"\r\n              v-model=\"title\"\r\n              class=\"blog-editor-input\"\r\n              size=\"medium\"\r\n              placeholder=\"文章标题\"></el-input>\r\n    <h3 class=\"blog-editor-title\">内容</h3>\r\n    <div id=\"blog-editor-textarea\">\r\n\r\n    </div>\r\n    <div class=\"blog-editor-tags\">\r\n      <h3 class=\"blog-editor-title\">分类选择</h3>\r\n      <el-radio-group v-model=\"column\">\r\n        <el-radio-button v-for=\"(item) in columns\"\r\n                         :key=\"item.id\"\r\n                         :label=\"item.id\">{{item.name}}</el-radio-button>\r\n      </el-radio-group>\r\n    </div>\r\n    <div class=\"blog-editor-button\">\r\n      <el-button type=\"primary\"\r\n                 @click=\"emptyEditor\">重置</el-button>\r\n      <el-button type=\"primary\"\r\n                 @click=\"submitEditor\">提交</el-button>\r\n    </div>\r\n\r\n  </el-card>\r\n</template>\r\n\r\n<script>\r\nimport wangEditor from 'wangeditor'\r\nexport default {\r\n  name: 'Editor',\r\n  components: {\r\n\r\n  },\r\n  data () {\r\n    return {\r\n      title: '',\r\n      content: '',\r\n      editor: null,\r\n      columns: [],\r\n      column: ''\r\n    }\r\n  },\r\n  mounted () {\r\n    const editor = new wangEditor(`#blog-editor-textarea`)\r\n    editor.config.onchange = (newHtml) => {\r\n      this.content = newHtml\r\n    }\r\n    editor.config.uploadImgServer = URL\r\n    editor.config.uploadImgMaxSize = 5 * 1024 * 1024 // 5M\r\n    editor.config.uploadImgAccept = ['jpg', 'jpeg', 'png', 'gif', 'bmp']\r\n    editor.config.uploadImgMaxLength = 5\r\n    editor.config.uploadFileName = 'file'\r\n    editor.config.uploadImgHeaders = {\r\n      'Authorization': `Bearer ${this.$store.state.token}`,\r\n    }\r\n    editor.create()\r\n    this.editor = editor\r\n  },\r\n  created () {\r\n    this.getColumns()\r\n  },\r\n  computed: {\r\n\r\n  },\r\n  watch: {\r\n\r\n  },\r\n  methods: {\r\n    async getColumns () {\r\n      try {\r\n        let columns = await this.$api({ type: 'column' })\r\n        this.columns = columns.list.map(item => {\r\n          return {\r\n            id: item.id,\r\n            name: item.name\r\n          }\r\n        })\r\n        this.column = this.columns[0].id\r\n      } catch (err) {\r\n        console.log(err)\r\n      }\r\n    },\r\n    submitEditor () {\r\n      if (!this.validateEditor()) {\r\n        return\r\n      }\r\n      this.postEditorData()\r\n    },\r\n    emptyEditor () {\r\n      this.editor.txt.clear()\r\n    },\r\n    validateEditor () {\r\n      if (this.title.trim().length === 0) {\r\n        this.$notify.error({\r\n          title: '错误',\r\n          message: '请填写标题'\r\n        })\r\n        this.$refs.title.focus()\r\n        return false\r\n      }\r\n      if (this.content.trim().length === 0) {\r\n        this.$notify.error({\r\n          title: '错误',\r\n          message: '内容不能为空'\r\n        })\r\n        return false\r\n      }\r\n      return true\r\n    },\r\n    async postEditorData () {\r\n      let postData = {\r\n        title: this.title,\r\n        column: this.column,\r\n        content: this.content,\r\n        cover: this.content.match(/src=\"([^\"']*)\"/)?.[1] || undefined\r\n      }\r\n      try {\r\n        this.$api({ type: 'postArticle', data: postData })\r\n        this.$notify.success({\r\n          title: '成功',\r\n          message: '文章提交成功'\r\n        })\r\n        this.$router.push('/index')\r\n      } catch (error) {\r\n        this.$notify.error({\r\n          title: '错误',\r\n          message: '提交失败'\r\n        })\r\n      }\r\n    }\r\n  },\r\n  beforeDestroy () {\r\n    // 调用销毁 API 对当前编辑器实例进行销毁\r\n    this.editor.destroy()\r\n    this.editor = null\r\n  }\r\n}\r\n</script>\r\n<style lang='stylus'>\r\n.blog-editor-title\r\n  padding 12px 0\r\n  font-size 16px\r\n.el-input--medium.blog-editor-input\r\n  font-size 18px\r\n  line-height 38px\r\n  height 38px\r\n.blog-editor-button\r\n  padding-top 20px\r\n  display flex\r\n  justify-content space-around\r\n  & button\r\n    flex 1\r\n    line-height 22px\r\n    font-size 18px\r\n#blog-editor-textarea\r\n  text-align left\r\n</style>"],"sourceRoot":"src/views"}]}