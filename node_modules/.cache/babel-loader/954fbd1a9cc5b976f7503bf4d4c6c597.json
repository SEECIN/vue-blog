it('SET_TOKEN')\n      dispatch('getUserInfo')\n    },\n    async logout ({ commit }) {\n      commit('DELETE_TOKEN')\n    },\n    async getUserInfo ({ commit ,state}) {\n      try {\n        let userInfo = await http({ type: 'getUserInfo'})\n        state.loginUsers.push(userInfo._id)\n        commit('SET_USERINFO', userInfo)\n      } catch (err) {\n        console.log(err)\n      }\n    }\n  },\n  modules: {\n    modal,likes\n  }\n})\n"]}]}